{
  "version": 2,
  "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
  ],
  "installCommand":"./scripts/setup.sh && yarn install",
  "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ]
      }
  ]
}